// BU2_ugxplo10 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo10;
	FRM_GetMyFrame(ugxplo10);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo10trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo10, 0, 3000);
	FRM_CreateParticle(51, ugxplo10);
	SetWhenever(detector, true);
	GoTo end;
}
// -------------------------------------------------------------------------------------------------------------
Label end: