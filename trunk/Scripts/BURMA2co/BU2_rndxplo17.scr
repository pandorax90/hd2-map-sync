// BU2_rndxplo17 - explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame rndxplo17;
	FRM_FindFrame(rndxplo17, "rndxplo17");
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
OnSignal(1)
{
If(_PlayerInRange(50))
{
		PlaySound(6, 8);
		Delay(700);
	Block
	{
		PlaySound(6, 2);
		FRM_CreateParticle(48, rndxplo17);
		MakeExplosion(rndxplo17, 800000, 15000);
	}
}
}