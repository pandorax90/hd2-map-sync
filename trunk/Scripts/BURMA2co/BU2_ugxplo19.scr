// BU2_ugxplo19 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo19;
	FRM_GetMyFrame(ugxplo19);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo19trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo19, 0, 3000);
	FRM_CreateParticle(51, ugxplo19);
	SetWhenever(detector, true);
	GoTo end;
}
// -------------------------------------------------------------------------------------------------------------
Label end: