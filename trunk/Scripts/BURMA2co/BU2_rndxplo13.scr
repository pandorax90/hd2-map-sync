// BU2_rndxplo13 - explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame rndxplo13;
	FRM_FindFrame(rndxplo13, "rndxplo13");
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
OnSignal(1)
{
If(_PlayerInRange(50))
{
		PlaySound(6, 8);
		Delay(700);
	Block
	{
		PlaySound(6, 2);
		FRM_CreateParticle(48, rndxplo13);
		MakeExplosion(rndxplo13, 800000, 15000);
	}
}
}