// BU2_ugxplo03 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo03;
	FRM_GetMyFrame(ugxplo03);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo03trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo03, 0, 3000);
	FRM_CreateParticle(51, ugxplo03);
	SetWhenever(detector, true);
	GoTo end;
}
// -------------------------------------------------------------------------------------------------------------
Label end: