// BU2_ugxplo20 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo20;
	FRM_GetMyFrame(ugxplo20);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo20trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo20, 0, 3000);
	FRM_CreateParticle(51, ugxplo20);
	SetWhenever(detector, true);
	GoTo end;
}
// -------------------------------------------------------------------------------------------------------------
Label end: