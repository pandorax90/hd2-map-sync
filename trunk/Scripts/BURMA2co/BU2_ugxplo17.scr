// BU2_ugxplo17 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo17;
	FRM_GetMyFrame(ugxplo17);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo17trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo17, 0, 3000);
	FRM_CreateParticle(51, ugxplo17);
	SetWhenever(detector, true);
	GoTo end;
}
// -------------------------------------------------------------------------------------------------------------
Label end: