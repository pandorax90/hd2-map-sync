frame CUDLIK;	 		FRM_FindFrame(CUDLIK, "m_radiog_.tlac_hide");		// cudlik
frame objimka;		FRM_FindFrame(objimka, "m_radiog_.objimka");		// objimka

FRM_SetOn(CUDLIK, True);	// S/L inicializace
