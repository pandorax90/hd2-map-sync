// BU2_obj_scrass - objectives script assigner
// -------------------------------------------------------------------------------------------------------------
Frame obj;
	FRM_FindFrame(obj, "BU2_objective");
Integer gametype;
	gametype=0;
// -------------------------------------------------------------------------------------------------------------
gametype=_SPGetGameType();

If((gametype==3) OR (gametype==7))
{
	ScriptAssign(obj, "BU2_objective2");
	printf("assigned bloodshed script BU2_objectives2");
	GoTo end;
}
ScriptAssign(obj, "BU2_objective");
printf("assigned normal mission script BU2_objectives");

Label end: