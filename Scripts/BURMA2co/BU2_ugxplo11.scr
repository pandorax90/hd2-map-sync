// BU2_ugxplo11 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo11;
	FRM_GetMyFrame(ugxplo11);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo11trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo11, 0, 3000);
	FRM_CreateParticle(51, ugxplo11);
	SetWhenever(detector, true);
	GoTo end;
}
// -------------------------------------------------------------------------------------------------------------
Label end: