// BU2_ugxplo13 - underground explosion effect
// -------------------------------------------------------------------------------------------------------------
// Variables definition
Frame ugxplo13;
	FRM_GetMyFrame(ugxplo13);
// -------------------------------------------------------------------------------------------------------------
// Whenever keywords and signals
Whenever detector(_SignalReceived(1))	// received from ugxplo13trig
{
	PlaySound(6, 5);
	MakeExplosion(ugxplo13, 0, 3000);
	FRM_CreateParticle(51, ugxplo13);
	SetWhenever(detector, true);
	GoTo end;
}
