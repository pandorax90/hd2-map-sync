// Spawnzone 2
//----------------------------------------------

Block
{
Frame Tiger;		FRM_FindFrame(Tiger,"A3_Tiger_01");
Frame Sp1;			FRM_FindFrame(Sp1,"Spawnzone1");
}

//----------------------------------------------

Whenever Spawn_TurnOn (_Actor_GetState(Tiger) == 0)
{
