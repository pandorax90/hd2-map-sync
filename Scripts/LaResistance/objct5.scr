// Obj 5 - Kill All Enemy
//----------------------------------------------------------

Block
{
SetObjectiveStatus(5, 0);
}

Whenever AED (_AllEnemiesDead())
{
SUBTITLES_SetOn(1);
SUBTITLES_SetText(07991406);
SetObjectiveStatus(5, 1);
Delay(4000);
SUBTITLES_SetOn(0);
EndScript();
}


