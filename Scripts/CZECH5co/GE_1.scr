HUMAN_SETMODE_Stand();
HUMAN_SETMODE_Walk();

OnAlarm()
	{
	HUMAN_Stop();
	HUMAN_SETMODE_Crouch();
	HUMAN_SETAIMODE_Aggressive();
	HUMAN_SetSniper(1,5);
	EndScript();
	}

OnAlarmDone()
	{
	HUMAN_SetAlarm(false);
	HUMAN_SETMODE_Stand();
	HUMAN_WeaponOnArm(false);
	HUMAN_Move("GE_1");
	}

OnCutscene(2)
	{
	HUMAN_Suspend(true);
	}

OnCutsceneDone(2)
	{
